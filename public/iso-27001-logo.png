  <link rel="stylesheet" type="text/css" href="/modules/isoorg-template/css/wood.full.min.css"/>
  <script type="text/javascript" src="/modules/isoorg-template/javascript/wood.full.min.js"></script>
  <link rel="shortcut icon" href="/modules/isoorg-template/img/iso/favicon/red/favicon.ico">
  <title>404 - Page not found</title>
  <script type="module">
    let isRender = false;
    let searchURL = '/search.html';
    const renderURI = '/cms/render/default';
    let pathname = window.location.pathname;
    if (pathname.startsWith(renderURI)) {
        isRender = true;
    }
    if (pathname.replace(renderURI, '').startsWith('/fr')) {
        searchURL = '/fr' + searchURL;
    }
    else if (pathname.replace(renderURI, '').startsWith('/ru')) {
        searchURL = '/ru' + searchURL;
    }
    if (isRender) {
        searchURL = renderURI + searchURL;
    }
    $(document).on('submit', 'form.isosearch', function(e) {
        e.preventDefault();
        var val = $(this).find('input[type="search"]').val();
        // Get rid of "."
        val = val.replace(/^\.$/, "");
        // Get rid of ".."
        val = val.replace(/^\.\.$/, "");
        // Get rid of every "%"
        val = val.replace(/%/g, "");
        val = val.trim();
        if (val !== '') {
            location.replace(searchURL + '?q=' + val);
        }
    });
</script>
</head>
<body role="main" id="content" aria-label="Content" class="acer error-page" onLoad="if (history.length > 1) { document.getElementById('backLink').style.display=''; }" tabindex="-1">
